function birthdayCakeCandles(candles) {
    
    let aux = 0;
    let aux2 = 0;
    
    for(let i=0; i < candles.length; i++){
        if( aux2 < candles[i] ){
            aux2 = candles[i];
            aux = 1;
        } else if(aux2 === candles[i]){
            aux = aux + 1;
        }
    }
    return aux;
}
